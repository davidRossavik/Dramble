{
  "expo": {
    "name": "Dramble",
    "slug": "Dramble",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/textLogo.png",
    "scheme": "dramble",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dramble.app",
      "buildNumber": "5",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/textLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/textLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/textLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "image": "./assets/images/textLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bace5e82-a533-4642-86ff-d3fe5d84ef4d"
      }
    }
  }
}
